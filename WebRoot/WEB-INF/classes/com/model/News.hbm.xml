<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

	<class name="News" table="news">
		<id name="newsId">
			<generator class="native"/>
		</id>
		<property name="passageHeadLine"/>
		<property name="newsAddedTime"/>
		<property name="newsWritter"/>
		<property name="newsComeFrom"/>
		<property name="newsImgAdd"/>
		<property name="newsReadCounts"/>
		<property name="passage"/>
	</class>
</hibernate-mapping>